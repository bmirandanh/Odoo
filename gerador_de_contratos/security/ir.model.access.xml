<odoo>
  <data>

    <!-- Definição do modelo Gerador de Contratos -->
    <record id="model_gerador_de_contratos" model="ir.model">
      <field name="name">Gerador de Contratos</field>
      <field name="model">x_gerador_de_contratos.model_gerador_de_contratos</field>
      <field name="state">manual</field>
      <field name="transient" eval="False"/>
      <field name="field_id" eval="[(6, 0, [])]"/>
    </record>

    <record id="group_gerador_de_contratos" model="res.groups">
      <field name="name">Grupo Gerador de Contratos</field>
      <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
    </record>


  </data>
</odoo>
