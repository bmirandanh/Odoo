<odoo>
  <data>
    <record id="model_gerador_de_contratos" model="ir.model">
      <field name="name">Gerador de Contratos</field>
      <field name="model">x_gerador_de_contratos.model_gerador_de_contratos</field>
      <field name="state">manual</field>
      <field name="transient" eval="False"/>
      <field name="field_id" eval="[(6, 0, [])]"/>
    </record>

    <record id="group_gerador_de_contratos" model="res.groups">
      <field name="name">Grupo Gerador de Contratos</field>
      <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
    </record>

    <record id="access_gerador_de_contratos" model="ir.model.access">
      <field name="name">Access Gerador de Contratos</field>
      <field name="model_id" ref="model_gerador_de_contratos"/>
      <field name="group_id" ref="group_gerador_de_contratos"/>
      <field name="perm_read" eval="1"/>
      <field name="perm_write" eval="1"/>
      <field name="perm_create" eval="1"/>
      <field name="perm_unlink" eval="1"/>
    </record>
  </data>
</odoo>
